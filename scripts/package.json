    "*": {
      "plugins": [
        "./dist/plugins/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@types/node": "17.0.35",
    "@types/fs-extra": "9.0.13",
    "@yarnpkg/lockfile": "1.1.0",
    "get-folder-size": "2.0.1",
    "axios": "0.27.2",
    "execa": "5.1.1",
    "prettier": "2.6.2",
    "gzip-size": "6.0.0",
    "fast-glob": "3.2.11",
    "consola": "2.15.3",
    "fs-extra": "10.1.0",
    "typescript": "^5"
  }
}
