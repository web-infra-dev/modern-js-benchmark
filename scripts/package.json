{
  "name": "@modern-js/benchmark-scripts",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run build && node ./dist/main.js",
    "dev": "tsc --watch",
    "build": "rm -rf ./dist && tsc"
  },
  "exports": {
    "./plugins": "./dist/plugins/index.js"
  },
